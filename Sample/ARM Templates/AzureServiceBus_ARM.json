{
    "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "namespaces_MyAzureServiceBusSample_name": {
            "defaultValue": "MyAzureServiceBusSample",
            "type": "String"
        },
        "topics_topic1_name": {
            "defaultValue": "topic1",
            "type": "String"
        },
        "subscriptions_sub1_name": {
            "defaultValue": "sub1",
            "type": "String"
        },
        "location_name": {
            "defaultValue": "Central India",
            "type": "String"
        }
      
    },
    "variables": {},
    "resources": [
        {
            "type": "Microsoft.ServiceBus/namespaces",
            "sku": {
                "name": "Standard",
                "tier": "Standard"
            },
            "name": "[parameters('namespaces_MyAzureServiceBusSample_name')]",
            "apiVersion": "2017-04-01",
            "location": "[parameters('location_name')]",
            "tags": {},
            "scale": null,
            "dependsOn": []
        },
        {
            "type": "Microsoft.ServiceBus/namespaces/topics",
            "name": "[concat(parameters('namespaces_MyAzureServiceBusSample_name'), '/', parameters('topics_topic1_name'))]",
            "apiVersion": "2017-04-01",
            "location": "[parameters('location_name')]",
            "scale": null,
            "dependsOn": [
                "[resourceId('Microsoft.ServiceBus/namespaces', parameters('namespaces_MyAzureServiceBusSample_name'))]"
            ]
        },
        {
            "comments": "Generalized from resource: '/subscriptions/02a0c632-86b4-4972-8b2a-c20752d297f8/resourcegroups/MyFunctionAppSample/providers/Microsoft.ServiceBus/namespaces/MyAzureServiceBusSample/topics/topic1/subscriptions/sub1'.",
            "type": "Microsoft.ServiceBus/namespaces/topics/subscriptions",
            "name": "[concat(parameters('namespaces_MyAzureServiceBusSample_name'), '/', parameters('topics_topic1_name'), '/', parameters('subscriptions_sub1_name'))]",
            "apiVersion": "2017-04-01",
            "location": "[parameters('location_name')]",
            "scale": null,
            "dependsOn": [
                "[resourceId('Microsoft.ServiceBus/namespaces', parameters('namespaces_MyAzureServiceBusSample_name'))]",
                "[resourceId('Microsoft.ServiceBus/namespaces/topics', parameters('namespaces_MyAzureServiceBusSample_name'), parameters('topics_topic1_name'))]"
            ]
        }
       
        
    ]
}